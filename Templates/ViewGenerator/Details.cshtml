@inherits Microsoft.VisualStudio.Web.CodeGeneration.Templating.RazorTemplateBase
@using Microsoft.VisualStudio.Web.CodeGeneration.EntityFrameworkCore
@using System.Collections.Generic
@using System.Linq
@{
    //System.Diagnostics.Debugger.Launch();
    //System.Diagnostics.Debugger.Break();

    System.Reflection.MethodInfo getGetEntityInfoMethod = base.Model.ModelMetadata.ModelType.GetMethod("GetEntityInfo");
    object instance = System.Activator.CreateInstance(base.Model.ModelMetadata.ModelType);
    var instanceinfo = getGetEntityInfoMethod.Invoke(instance,null);
    System.Reflection.MethodInfo getAttributeInfoMethod = base.Model.ModelMetadata.ModelType.GetMethod("GetAttributeInfo");
    var attributeinfos = getAttributeInfoMethod.Invoke(instance,null);
    
}
